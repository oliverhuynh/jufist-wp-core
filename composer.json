{
  "name": "jufist/wp-core",
  "description": "Standalone/Drupal/Wordpress Core. The plugin allow you to dev Standalone/Drupal/WP plugins faster",
  "version": "1.0.1",
  "keywords": [
    "wordpress",
    "drupal",
    "standalone",
    "base",
    "prototype"
  ],
  "homepage": "https://github.com/jufist/wp-core",
  "license": "MIT",
  "require": {
    "php": ">=5.6.20"
  },
  "require": {
	        "johnpbloch/wordpress-core": ">=5.4"
	},
  "authors": [
    {
      "name": "Oliver Huynh",
      "email": "oliver@jufist.com",
      "homepage": "https://jufist.com"
    }
  ],
  "autoload": {
    "files": [
        "src/init.php"
    ],
	  "psr-4": {
            "Jufist\\": "src/Jufist/"
        },
    "classmap": [
        "src/Jufist/WpCore/WpCore.php",
        "src/Jufist/WpCore/AloneCore.php",
      "src/Jufist/WpCore/Core.php"
    ]
  },
	"repositories": [
	        {
	            "type": "composer",
	            "url": "https://wpackagist.org"
	        }
	]
}
